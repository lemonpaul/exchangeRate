$('#rates').replaceWith("<%= j render :template => 'rates/index' %>");
if ($('#new_link').is(":visible"))
{
  $('#triggers').replaceWith("<%= j render :template => 'triggers/index' %>");
}
if ($('#rate_count').is(":visible"))
{
  $('#rate_count').replaceWith("<%= j render :partial => 'logs/header' %>");
  $('#rate_count').show();
}
if ($('#logs').is(":visible"))
{
  $('#logs').replaceWith("<%= j render :partial => 'logs/logs' %>");
  $('#logs').show();
}
if ($('#chart_canvas').is(":visible"))
{
  $('#script').replaceWith("<%= j render :partial => 'charts/script' %>");
}