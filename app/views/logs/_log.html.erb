<tr id="log_<%= log.id %>" class="log">
  <td><p><%= l log.created_at, format: :short %></p></td>
  <% if log.currency == 0 %>
    <td><%= t(:usd) %></td>
  <% else %>
    <td><%= t(:eur) %></td>
  <% end %>
  <% if log.operation == 0 %>
    <td><%= t(:sell) %></td>
  <% else %>
    <td><%= t(:buy) %></td>
  <% end %>
  <td><%= "%.2f" % log.rate %></td>
  <td><%= link_to t(:delete), 
    log_path(log),
    method: :delete, 
    remote: true, 
    class: 'badge' %>
  </td>
</tr>